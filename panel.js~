var panel=function(){
ret={};
var mraa=require("mraa");
var candidates=[];
var offset=13;
for(var i=0;i<3;i++){
    candidates[i]=new mraa.Gpio(offset);
    candidates[i].isr(mraa.EDGE_RISING,function(i){
    console.log("pin number "+i+" is pressed");
 };
 offset--;
};
ret.watch=function(cb){
    console.log("watching panel");
    for(var i=0;i<3;i++){
    cb(i);
   });
};
return ret;
};
module.exports=panel();

